{
  "name": "anyforms-front",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://anyforms.ru",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "react-hot-toast": "^2.4.1",
    "axios": "^1.6.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "pnpm build",
    "deploy": "gh-pages -d build",
    "postbuild": "cp CNAME build/ && cp 404.html build/",
    "api": "swagger-typescript-api generate -p https://anyforms.up.railway.app/v3/api-docs -o ./src/shared/api -n api.gen.ts --axios --union-enums",
    "dev-api": "swagger-typescript-api generate -p http://localhost:8090/v3/api-docs -o ./src/shared/api -n api.gen.ts --axios --union-enums"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^6.3.0",
    "swagger-typescript-api": "^13.0.3"
  },
  "packageManager": "pnpm@9.15.1+sha512.1acb565e6193efbebda772702950469150cf12bcc764262e7587e71d19dc98a423dff9536e57ea44c49bdf790ff694e83c27be5faa23d67e0c033b583be4bfcf"
}


